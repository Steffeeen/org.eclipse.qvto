<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         name="JDT blackbox implementation provider"
         point="org.eclipse.m2m.qvt.oml.blackboxProvider">
      <provider
            class="org.eclipse.m2m.internal.qvt.oml.runtime.jdt.blackbox.JdtBlackboxProvider">
      </provider>
   </extension>

    <extension
          point="org.eclipse.debug.core.statusHandlers">
       <statusHandler
             class="org.eclipse.m2m.internal.qvt.oml.runtime.jdt.blackbox.NavigateToJavaElementHandler"
             code="500"
             id="org.eclipse.m2m.qvt.oml.runtime.jdt.navigateToJavaElementHandler"
             plugin="org.eclipse.m2m.qvt.oml.editor.ui"/>       
    </extension>    

</plugin>
